            debugger;
            $('input[type="checkbox"]').each(function () {
                debugger;
                var id = $(this).attr("id");
                var name = $(this).attr("name");

                if ($(this).is(":checked")) {
                    $("input[name='" + name + "']").val("1");
                } else {
                    $("input[name='" + name + "']").val("0");
                }
            });
            //e.preventDefault();
